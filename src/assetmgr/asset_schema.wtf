AssetType Occlusion {
	desc: "Occlusion information needed by the game at runtime to determine which objects are visible.\n\nThe playable space of a level is divided into 4x4x4 cubes called octants that dictate which objects are visible at a given time."
	
	IntegerAttribute memory_budget {
		desc: "The maximum size of the computed collision database in bytes, including both the octant lookup tree and the visibility masks."
		required: false
		games: [1 2 3 4]
	}
	
	FileReferenceAttribute octants {
		desc: "A path to a CSV file containing a list of all the octants for which occlusion should be generated. Each line is a list of 3 integers X, Y and Z separated by commas. Each coordinate must be multiplied by 4 to obtain world coordinates."
		required: true
		games: [1 2 3 4]
	}
	
	FileReferenceAttribute grid {
		desc: "The raw occlusion grid data used by the game. Either unpacked by the build tool or generated by the visibility tool."
		required: true
		games: [1 2 3 4]
	}
	
	FileReferenceAttribute mappings {
		desc: "Mappings between the instances in the gameplay file and the occlusion grid. Either unpacked by the build tool or generated by the visibility tool."
		required: true
		games: [1 2 3 4]
	}
}

