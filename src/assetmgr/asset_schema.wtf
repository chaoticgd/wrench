		desc: "The materials used by this class."
		allowed_types: ["Collection"]
		required: true
		games: [1 2 3 4]
		Child child {
			allowed_types: ["Material"]
		}
	}
	
	Child static_collision {
		desc: "The collision to be be built into the main static level collision."
		allowed_types: ["Collision"]
		required: false
		games: [1 2 3 4]
	}
	
	Child editor_mesh {
		desc: "The mesh shown in the editor. This is currently used as a hack since we haven't got the moby exporter working, but in the future it could be used for invisible mobies."
		allowed_types: ["Mesh"]
		required: false
		games: [1 2 3 4]
	}
	
